
void* PyErr_Format(void *excType, char *format, ...)
{
    va_list ap;
    va_start(ap, format);
    char buf[512];
    vsnprintf(buf, sizeof(buf), format, ap);
    va_end(ap);

    void* (*mgd_PyErr_Format)(void*, char*) = (void* (*)(void*, char*))(jumptable[%d]);
    return mgd_PyErr_Format(excType, buf);
}
